{
    "name": "html5-scaffold",
    "root": "build",
    "clean_urls": true,
    "error_page": "error.html",
    "routes": {
        "/": "/index.html",
        "/mock-services/resource-collection": "/mock-services/resource-collection/_collection.json"
    },
    "proxy": {
        "api": {
            "origin": "https://www.google.com",
            "headers": {
                "Accept": "application/json"
            },
            "cookies": false,
            "timeout": 30
        }
    }

}

